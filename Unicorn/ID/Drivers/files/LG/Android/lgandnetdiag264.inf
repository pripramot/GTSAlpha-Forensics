;-----------------------------------------------------------------------------
; Installation instructions
;-----------------------------------------------------------------------------

[Lgsi.Install.nt]
AddReg		= Lgsi.AddReg,Lgsi.NT.AddReg
CopyFiles	= CopyFiles, CopyFiles.Win2K

;-----------------------------------------------------------------------------
;  Registry
;-----------------------------------------------------------------------------

; common registry entries 
[Lgsi.AddReg]
HKR,,NTMPDriver,,lgandnetdiag264.sys

; only for NT...
[Lgsi.NT.AddReg]
HKR,,PortSubClass,1,01
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

;-----------------------------------------------------------------------------
;  Copy lists
;-----------------------------------------------------------------------------
[CopyFiles]

[CopyFiles.Win2K]
lgandnetdiag264.sys

;-----------------------------------------------------------------------------
;  NT Services installation
;-----------------------------------------------------------------------------
[Lgsi.Install.NT.Services]
AddService = AndNetDiag2, 0x00000002, Lgsi.Install.AddService

[Lgsi.Install.AddService]
DisplayName	= %Lgsi.Service.Name%
Description	= %Lgsi.Service.Desc%
ServiceType	= 1			;%SERVICE_KERNEL_DRIVER%
StartType	= 3			;%SERVICE_DEMAND_START%
ErrorControl	= 1			;%SERVICE_ERROR_NORMAL%	
ServiceBinary	= %12%\lgandnetdiag264.sys

