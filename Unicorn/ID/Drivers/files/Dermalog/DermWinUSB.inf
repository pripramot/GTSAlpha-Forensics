; ================= Installation LF x86 =================
[DRM_Install_LF.NTx86]
	Include = winusb.inf
	Needs = WINUSB.NT

[DRM_Install_LF.NTx86.Services]
	Include = winusb.inf
	AddService = WUDFRd, 0x000001f8, DRM_WUDFRD_ServiceInstall
	AddService = WinUSB, 0x000001fa, DRM_WinUSB_ServiceInstall

[DRM_Install_LF.NTx86.Wdf]
	KmdfService = WINUSB, DRM_WinUsb_Install
	UmdfService = UmdfSection, DRM_UmdfSection_install
	UmdfServiceOrder = UmdfSection

[DRM_Install_LF.NTx86.HW]
	AddReg = DRM_AddReg_Interface_LF

[DRM_Install_LF.NTx86.CoInstallers]
	AddReg = DRM_CoInstallers_AddReg
	CopyFiles = DRM_CoInstallers_CopyFiles

