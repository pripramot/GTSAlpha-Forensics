; =================== Driver Base =====================
[DRM_WUDFRD_ServiceInstall]
	ServiceType     = %SERVICE_KERNEL_DRIVER%
	StartType       = %SERVICE_DEMAND_START%
	ErrorControl    = %SERVICE_ERROR_NORMAL%
	ServiceBinary   = %12%\WUDFRd.sys

[DRM_WinUSB_ServiceInstall]
	DisplayName     = %WinUSB_SvcDesc%
	ServiceType     = %SERVICE_KERNEL_DRIVER%
	StartType       = %SERVICE_DEMAND_START%
	ErrorControl    = %SERVICE_ERROR_NORMAL%
	ServiceBinary   = %12%\WinUSB.sys
	
[DRM_LibUSBK_AddService]
	DisplayName		= "libusbK USB Driver 04/27/2014 - 3.0.7.0"
	ServiceType		= 1
	StartType		= 3
	ErrorControl	= 1
	ServiceBinary	= %12%\libusbK.sys
	AddReg 			= DRM_LibUSBK_AddReg

[DRM_LibUSBK_AddReg]
	HKR,"Parameters","DebugLevel",0x10001,4	
	
[DRM_WinUSB_Install]
	KmdfLibraryVersion = 1.9

[DRM_LibUSBK_Install]
	KmdfLibraryVersion = 1.9
	
[DRM_UmdfSection_install]
	UMDFLibraryVersion = 1.9.0
	ServiceBinary      = %12%\umdf\winusb.dll
	DriverCLSID        = {0AE2EB4A-E710-4d77-B4CE-2E7A4861585C}	

; =========== CoInstaller ===============
[DRM_CoInstallers_LF1_AddReg]
	HKR,,CoInstallers32,0x00010000,"WdfCoInstaller01009.dll,WdfCoInstaller"

[DRM_CoInstallers_LF1_CopyFiles]
	WdfCoInstaller01009.dll

[DRM_CoInstallers_AddReg]
	HKR,,CoInstallers32,0x00010000,"WinUSBCoInstaller2.dll","WUDFUpdate_01009.dll","WdfCoInstaller01009.dll,WdfCoInstaller"

[DRM_CoInstallers_CopyFiles]
	WinUSBCoInstaller2.dll
	WdfCoInstaller01009.dll
	WUDFUpdate_01009.dll
	
