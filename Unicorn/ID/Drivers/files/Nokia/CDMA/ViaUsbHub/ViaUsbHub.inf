; ==============================================================
; =================== Strings Sections =========================
; ==============================================================
[Strings]
VIATelecom         = "Nokia Inc"
NOKIA1508 = "Nokia 1508 Device #2"
NOKIA1508I = "Nokia 1508i Device #2"
SOURCE_DISKS_NAMES = "VIA Telecom\\VIA CBP USBD"
Nokia              = "Nokia Inc"
VIATelecomHub = "VIA Telecom USB HUB"         
