; ==============================================================
; ================= Version/Class Sections =====================
; ==============================================================
[Version]
CatalogFile = ViaTelecomFile.cat
signature   = "$CHICAGO$"
Class       = USB
ClassGUID   = {36FC9E60-C465-11CF-8056-444553540000} 
Provider    = %Nokia%
DriverVer   = 04/21/2009,2.0.13

      
; ==============================================================
; =================== Table of Contents ========================
; ==============================================================
[Manufacturer]
%VIATelecom% = DeviceList,NTia64,NTamd64
      

; ==============================================================
; ================= Control Flags Sections =====================
; ==============================================================
[ControlFlags]
ExcludeFromSelect = USB\VID_0421&PID_0162
ExcludeFromSelect = USB\VID_0421&PID_018F


; ==============================================================
; ================== Driver/Device Sections ====================
; ==============================================================
[DeviceList]
%NOKIA1508% = Composite.Dev, USB\VID_0421&PID_0162
%NOKIA1508I% = Composite.Dev, USB\VID_0421&PID_018F

[DeviceList.NTia64]
%NOKIA1508% = Composite.Dev, USB\VID_0421&PID_0162
%NOKIA1508I% = Composite.Dev, USB\VID_0421&PID_018F

[DeviceList.NTamd64]
%NOKIA1508% = Composite.Dev, USB\VID_0421&PID_0162
%NOKIA1508I% = Composite.Dev, USB\VID_0421&PID_018F


; ==============================================================
; ===================== Pre-Copy Section =======================
; ==============================================================



; ==============================================================
; =================== Copy Files Section =======================
; ==============================================================
[DestinationDirs]
CoCopyINF.CopyFiles = 11					;

[SourceDisksFiles]
cocpyinf.dll = 1,,

[SourceDisksNames]
1 = %SOURCE_DISKS_NAMES%



; =========================================================
; ---------------- Windows 98/ME Sections -----------------
; =========================================================
[Composite.Dev]
AddReg    = Hub.AddReg
include   = usb.inf


; ==============================================================
; ---------------- Windows 2000 & XP Sections ------------------
; ==============================================================
[Composite.Dev.NT]
AddReg    = StandardHub.AddReg.NT
include   = usb.inf
;CopyInf   = ViaUsbModem.inf, ViaUsbEts.inf

[standardhub.addreg.nt] ; Fix ERROR(1203) Section [standardhub.addreg.nt] not found.

;[Composite.Dev.NT.CoInstallers]
;AddReg    = CoCopyINF.AddReg
;CopyFiles = CoCopyINF.CopyFiles


