[Version]
Signature   = "$WINDOWS NT$"
Class       = Ports
Provider    = %Provider%
ClassGuid   = {4D36E978-E325-11CE-BFC1-08002BE10318}
;DriverVer updated to the minimum required to support the required OSs
;DriverVer   = 08/27/2008, 1.0.1.133
DriverVer   = 04/21/2009, 1.0.1.133
CatalogFile = nok_prt.cat

[ControlFlags]
ExcludeFromSelect = *

[SourceDisksNames]
1 = %DiskString%, "nok_prt.sys",,

[SourceDisksFiles]
nok_prt.sys = 1
nok_flt.sys = 1

[DestinationDirs]
