[NOKIA1.Install.nt]
AddReg    = NOKIA.AddReg
CopyFiles = NOKIA.Files.Ext.nt

[NOKIA1.Install.ntamd64]
AddReg    = NOKIA.AddReg
CopyFiles = NOKIA.Files.Ext.ntamd64

[NOKIA2.Install.nt]
AddReg    = Nokia.AddReg
CopyFiles = NOKIA.Files.Ext.nt

[NOKIA2.Install.ntamd64]
AddReg    = Nokia.AddReg
CopyFiles = NOKIA.Files.Ext.ntamd64

[NOKIA.AddReg]
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"
HKR,,LocationInformationOverride,,"NOKIA Mobile Technologies"

[NOKIA.Files.Ext.nt]
nok_prt.sys
nok_flt.sys

[NOKIA.Files.Ext.ntamd64]
nok_prt.sys
nok_flt.sys

[NOKIA1.Install.NT.HW]
AddReg = UpperFilterAddReg

[NOKIA2.Install.NT.HW]
AddReg = UpperFilterAddReg

[NOKIA1.Install.nt.Services]
AddService = nokiaprt, 0x00000002, NOKIA.Install.AddService.nt
AddService = nokiaflt,, NOKIA.Install.AddFilter.nt

[NOKIA1.Install.ntamd64.Services]
AddService = nokiaprt, 0x00000002, NOKIA.Install.AddService.ntamd64
AddService = nokiaflt,, NOKIA.Install.AddFilter.ntamd64

[NOKIA2.Install.nt.Services]
AddService = nokiaprt, 0x00000002, NOKIA.Install.AddService.nt
AddService = nokiaflt,, NOKIA.Install.AddFilter.nt

[NOKIA2.Install.ntamd64.Services]
AddService = nokiaprt, 0x00000002, NOKIA.Install.AddService.ntamd64
AddService = nokiaflt,, NOKIA.Install.AddFilter.ntamd64

[NOKIA.Install.AddService.nt]
DisplayName   = %Nokia.Port.ServiceDesc%
ServiceType   = 1
StartType     = 3
ErrorControl  = 1
ServiceBinary = %12%\nok_prt.sys

[NOKIA.Install.AddService.ntamd64]
DisplayName   = %Nokia.Port.ServiceDesc%
ServiceType   = 1
StartType     = 3
ErrorControl  = 1
ServiceBinary = %12%\nok_prt_64.sys

[NOKIA.Install.AddFilter.nt]
DisplayName   = %Nokia.Filter.ServiceDesc%
ServiceType   = 1
StartType     = 3
ErrorControl	= 1
ServiceBinary	= %12%\nok_flt.sys

[NOKIA.Install.AddFilter.ntamd64]
DisplayName   = %Nokia.Filter.ServiceDesc%
ServiceType   = 1
StartType     = 3
ErrorControl	= 1
ServiceBinary	= %12%\nok_flt_64.sys

[UpperFilterAddreg]
HKR,,"UpperFilters",0x00010000,"nokiaflt"

