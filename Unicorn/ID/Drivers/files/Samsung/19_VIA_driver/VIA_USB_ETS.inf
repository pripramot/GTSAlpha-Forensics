Signature   = "$CHICAGO$"
Provider    = %VENDOR%
Class       = USB
ClassGUID   = {36FC9E60-C465-11CF-8056-444553540000} 


; ==============================================================
; =================== Table of Contents ========================
; ==============================================================
[Manufacturer]
%VENDOR% = DeviceList,NTia64,NTamd64

; ==============================================================
; ================= Control Flags Sections =====================
