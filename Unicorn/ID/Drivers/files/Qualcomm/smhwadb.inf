[Version]
Signature="$WINDOWS NT$"
Class=AndroidUsbDeviceClass
ClassGuid={3F966BD9-FA04-4ec5-991C-D326973B5128}
Provider=%GOOG%
DriverVer=05/16/2011,1.1.0009.00000
CatalogFile.NTx86=smhwadb86.cat
CatalogFile.NTamd64=smhwadb64.cat

; ================= Class section =====================

[ClassInstall32]
Addreg=AndroidUsbClassReg

[AndroidUsbClassReg]
HKR,,,0,%ClassName%
HKR,,Icon,,-5

[DestinationDirs]
DefaultDestDir = 12
CoInstaller_CopyFiles = 11 ; Moved from duplicate [DestinationDirs] section

; ================= Device section =====================

[Manufacturer]
%MfgName%=Google,NTx86,NTamd64

; For Win2K
[Google]
; For Fastboot
;%HWFASTBOOT%=androidusb.Dev, USB\VID_18D1&PID_D00D ; Duplicated in 1\Android\Huawei\Huawei3\amd64\hw_goadb.inf
; Android Adapter 
;%HWADBNAME%=androidusb.Dev, USB\VID_05C6&PID_9025 ; Duplicated in 1\Android\CAT\amd64\smhwser.inf
%HWADBNAME%=androidusb.Dev, USB\VID_05C6&PID_9024
%HWADBNAME%=androidusb.Dev, USB\VID_05C6&PID_9025&MI_01
%HWADBNAME%=androidusb.Dev, USB\VID_05C6&PID_9024&MI_02
