[SourceDisksFiles.x86]
  qcusbser.sys = 1000,x86

[SourceDisksFiles.amd64]
  qcusbser.sys = 1000,amd64

[QcomSerialPort.NTx86]
; HUAWEI Ideos X6
%QcomDeviceC0001%  = QportInstall00, USB\VID_0489&PID_C000&MI_01
