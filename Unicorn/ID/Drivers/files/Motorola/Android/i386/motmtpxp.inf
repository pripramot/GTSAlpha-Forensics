[ControlFlags]
ExcludeFromSelect=*


[MTP]
Include = wpdmtp.inf
Needs   = WPD.MTP

[MTP.hw]
Include = wpdmtp.inf
Needs   = WPD.MTP.Registration

[MTP.Services]
Include = wpdmtp.inf
Needs   = WPD.MTP.Services

[MTP.CoInstallers]
Include = wpdmtp.inf
Needs = WPD.MTP.CoInstallers

[MTP.Wdf]
Include = wpdmtp.inf
Needs = WPD.MTP.Wdf
UmdfServiceOrder=WpdMtpDriver


[Strings]
Motorola              = "Motorola"
Motmtp.DeviceDesc     = "Motorola MTP Device"
Msft                 ="Microsoft" ; Moved from duplicate [Strings] section
MfgNameVendorModels  ="(Standard MTP-Compliant Device)" ; Moved from duplicate [Strings] section
GenericMTP.DeviceDesc="MTP Device" ; Moved from duplicate [Strings] section































