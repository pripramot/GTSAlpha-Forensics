[Version]
Signature           = "$Windows NT$"
Class               = Net
ClassGUID           = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider            = %Provider%
CatalogFile         = onetouchrndis.cat
DriverVer           =04/05/2012,1.2.5.0

[Manufacturer]
%onetouch% = onetouchDevices, NTx86, NTamd64

[onetouchDevices.NTx86]
%onetouch.VID_1BBB&PID_0004.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0004&MI_00
%onetouch.VID_1BBB&PID_0003.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0003&MI_00
%onetouch.VID_1BBB&PID_0010.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0010&MI_00
