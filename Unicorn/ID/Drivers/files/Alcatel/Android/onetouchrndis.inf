%onetouch.VID_1BBB&PID_0012.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0012&MI_00
%onetouch.VID_1BBB&PID_0013.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0013&MI_00
%onetouch.VID_1BBB&PID_0014.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0014&MI_00
%onetouch.VID_1BBB&PID_0015.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0015&MI_00
[onetouchDevices.NTamd64]
%onetouch.VID_1BBB&PID_0004.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0004&MI_00
%onetouch.VID_1BBB&PID_0003.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0003&MI_00
%onetouch.VID_1BBB&PID_0010.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0010&MI_00
%onetouch.VID_1BBB&PID_0011.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0011&MI_00
%onetouch.VID_1BBB&PID_0012.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0012&MI_00
%onetouch.VID_1BBB&PID_0013.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0013&MI_00
%onetouch.VID_1BBB&PID_0014.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0014&MI_00
%onetouch.VID_1BBB&PID_0015.Desc%    = onetouch.install.NT.5.1, USB\VID_1BBB&PID_0015&MI_00
[ControlFlags]
ExcludeFromSelect=*

; Windows LH specific sections -----------------------------------

[onetouch.install.NT.5.1]
Characteristics = 0x84   ; NCF_PHYSICAL + NCF_HAS_UI
BusType         = 15
include         = netrndis.inf
needs           = Usb_Rndis.ndi

[onetouch.install.NT.5.1.Services]
include     = netrndis.inf
needs       = Usb_Rndis.ndi.Services

; no sys copyfiles - the sys files are already in place

[Strings]
Provider    = "Alcatel"
onetouch     = "Alcatel"

onetouch.VID_1BBB&PID_0004.Desc="Alcatel Composite RNDIS Interface"
