S1317.ndi.DriverDesc = "Sony Ericsson Device 1317 USB Ethernet Emulation (NDIS)"
Ndis3Desc = "Sony Ericsson Device 1317 USB Ethernet Emulation (NDIS 3)"
Ndis5Desc = "Sony Ericsson Device 1317 USB Ethernet Emulation (NDIS 5)"
