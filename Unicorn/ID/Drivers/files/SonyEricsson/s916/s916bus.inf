[version]
signature="$WINDOWS NT$"
Class=USB
ClassGuid={36FC9E60-c465-11cf-8056-444553540000}
Provider=%Provider%
;DriverVer updated to the minimum required to support the required OSs
;DriverVer=09/05/2007,4.40.8.1
DriverVer=04/21/2009,4.40.8.1
CatalogFile=s916bus.cat

[ControlFlags]
ExcludeFromSelect = *

;-----------------------------------------------------------------------------
; Specify target paths...
;-----------------------------------------------------------------------------
[DestinationDirs]
DefaultDestDir                  = 11                    ; LDID_SYS
sed916Wdm.W95.CopyFiles       = 11                    ; LDID_SYS
sed916Wdm.NT.CopyFiles        = 12                    ; system32\drivers
PropPages.CopyFiles             = 11                    ; system

;-----------------------------------------------------------------------------
; Specify the install disks.  A bit unwieldy during development.
;-----------------------------------------------------------------------------
[SourceDisksNames.x86]
; diskid = description[, [tagfile] [, <unused>, subdir]]
1=%DiskString%,"s916bus.sys",,\i386


[SourceDisksNames.amd64]
; diskid = description[, [tagfile] [, <unused>, subdir]]
1=%DiskString%,"s916bus.sys",,\amd64



[SourceDisksFiles]
; filename = diskid[, [subdir] [, size]]
s916bus.sys = 1
s916wh95.sys = 1
s916whnt.sys = 1

;-----------------------------------------------------------------------------
; Table of Contents:  manufacturer, then devices.
;-----------------------------------------------------------------------------
[Manufacturer]
%Sony Ericcson%=sed916, NTamd64

;-----------------------------------------------------------------------------
; The devices
;-----------------------------------------------------------------------------
[sed916]
%sed916.CdcBus.Desc% = sed916Wdm.Install, USB\VID_0fce&PID_d0b5
%sed916.CdcBus.Desc% = sed916Wdm.Install, USB\VID_0fce&PID_d0c7


[sed916.NTamd64]
%sed916.CdcBus.Desc% = sed916Wdm.Install, USB\VID_0fce&PID_d0b5
%sed916.CdcBus.Desc% = sed916Wdm.Install, USB\VID_0fce&PID_d0c7


;-----------------------------------------------------------------------------
; How to install them:  On W9x, the [section] name is used directly.
; On NT, the system first tries [section.NT], then [section].
;-----------------------------------------------------------------------------

;-----------------------------------------------------------------------------
; Diagnostics Files CopyFiles List Section
;-----------------------------------------------------------------------------
[sed916.CopyFiles]

;-----------------------------------------------------------------------------
; XXX when we have a property page DLL, add it to the following section.
;-----------------------------------------------------------------------------
[PropPages.CopyFiles]

;-----------------------------------------------------------------------------
;       How to install the WDM driver on W95/W98
;-----------------------------------------------------------------------------
[sed916Wdm.Install]
CopyFiles       = sed916Wdm.W95.CopyFiles, sed916.CopyFiles, PropPages.CopyFiles
AddReg          = sed916Wdm.AddReg, sed916Wdm.W95.AddReg

;-----------------------------------------------------------------------------
;       How to install the WDM driver on NT
;-----------------------------------------------------------------------------
[sed916Wdm.Install.NT]
CopyFiles       = sed916Wdm.NT.CopyFiles
AddReg          = sed916Wdm.AddReg, sed916Wdm.NT.AddReg

;-----------------------------------------------------------------------------
;       How to start the WDM driver on NT
;-----------------------------------------------------------------------------
[sed916Wdm.Install.NT.Services]
AddService      = s916bus, 2, sed916Wdm.AddService

[sed916Wdm.AddService]
DisplayName             = %sed916.Service.Desc%
ServiceType             = 1     ; SERVICE_KERNEL_DRIVER
StartType               = 3     ; SERVICE_DEMAND_START
ErrorControl            = 1     ; SERVICE_ERROR_NORMAL
ServiceBinary           = %12%\s916bus.sys
LoadOrderGroup          = Base

;-----------------------------------------------------------------------------
;       WDM registry entries (all platforms)
;-----------------------------------------------------------------------------
[sed916Wdm.AddReg]


;-----------------------------------------------------------------------------
;       WDM registry entries (W95/98) (including how to start the driver)
;-----------------------------------------------------------------------------
[sed916Wdm.W95.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,s916bus.sys

;-----------------------------------------------------------------------------
;       WDM registry entries (NT only)
;-----------------------------------------------------------------------------
[sed916Wdm.NT.AddReg]

;-----------------------------------------------------------------------------
;       The WDM copy list for Win95 -- different because things go in
;       different places.  Also, we probably need to rename drivers 
;       during the install.
;-----------------------------------------------------------------------------
[sed916Wdm.W95.CopyFiles]
s916bus.sys
s916wh.sys,s916wh95.sys
s916wh95.sys
s916whnt.sys

;-----------------------------------------------------------------------------
;       The WDM copy list for NT -- different because things go in
;       different places.
;-----------------------------------------------------------------------------
[sed916Wdm.NT.CopyFiles]
s916bus.sys
s916wh.sys,s916whnt.sys
s916whnt.sys


;****************************************************************************
; Localizable Strings
;****************************************************************************
[strings]

;BEGIN ENG - US English Strings
Provider = "Sony Ericsson"
Sony Ericcson = "Sony Ericsson"
DiskString = "Sony Ericsson Device 916 Installation disk"

sed916.CdcBus.Desc = "Sony Ericsson Device 916"
sed916.Service.Desc = "Sony Ericsson Device 916 driver (WDM)"

CustomerDataPath = "Sony Ericsson Device 916"
DriverSet = "Sony Ericsson Device 916 Software"
UninstallFile = "S916Uninstall.exe"
McciOptions = "Uninstallable"


;END ENG - US English Strings

