;END ENG - US English Strings

;-----------------------------------------------------------------------------
; Following are symbolic constants which don't change per locale.
;-----------------------------------------------------------------------------
SEG_106.ndi5.guid = "se26CR\{2D65D62A-DCD5-4c9d-85A5-E24B5970EC84}"
SEG_106.ndi3.guid = "se26CR\{7D3822F8-BCDB-4fa1-B046-B311E2FC92C8}"


