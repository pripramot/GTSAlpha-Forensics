McciOptions = "Uninstallable"


;END ENG - US English Strings

;-----------------------------------------------------------------------------
; Following are symbolic constants which don't change per locale.
;-----------------------------------------------------------------------------
S716.ndi5.guid = "s716CR\{94efa48d-06ca-4af6-b21d-7b3755732a8e}"
S716.ndi3.guid = "s716CR\{84beb84f-69c1-49d5-a340-28355ef173ff}"


