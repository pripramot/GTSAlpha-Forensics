Provider = "Sony Ericsson"
S3017 = "Sony Ericsson"
DiskString = "Sony Ericsson Device 3017 USB Ethernet Emulation Installation Disk"

S3017.ndi.DriverDesc = "Sony Ericsson Device 3017 USB Ethernet Emulation (NDIS)"
Ndis3Desc = "Sony Ericsson Device 3017 USB Ethernet Emulation (NDIS 3)"
Ndis5Desc = "Sony Ericsson Device 3017 USB Ethernet Emulation (NDIS 5)"
S3017.Service.Desc = "Sony Ericsson Device 3017 USB Ethernet Emulation SEMC3017 (NDIS)"
